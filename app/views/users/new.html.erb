<div class="">
  <nav class="navbar-default navbar-fixed-top">
    <div class="col-xs-12">
      <div class="navbar-left nav-img">
        <%=image_tag("blue-tent-icon.png", class:"small-icon" )%>
      </div>
      <div class="navbar-right">
        <ul class="nav navbar-nav">
          <li><a href="../session/new">Login</a></li>
        </ul>
      </div>
    </div>
  </nav>

  <div>
    <%=image_tag('../theme.jpeg', class:'theme-bg') if Rails.env.production?%>
    <%=image_tag('theme.jpeg', class:'theme-bg') unless Rails.env.production?%>

    <form class="form-group signin-page-static" id="signinForm" action="<%= users_url %>" method="post">
      <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
      <label class="form-inline signin-modal-ele-font">Email: </label>
      <input class="form-control col-md-6 col-xs-6 col-sm-6 col-lg-6" type="email" name="user[email]"/>
      <br></br>
      <label class="top-buffer form-inline signin-modal-ele-font">Password: </label>
      <input class="form-control" type="password" name="user[password]"/>
      <br/>
      <button class="btn blue-btn text-center" type="submit">Sign Up</button>
    </form>
  </div>
</div>
